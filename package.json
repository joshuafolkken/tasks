{
	"name": "tasks",
	"private": true,
	"version": "0.22.1",
	"type": "module",
	"packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc",
	"scripts": {
		"preinstall": "npx only-allow pnpm && pnpm dlx @aikidosec/safe-chain setup-ci",
		"postinstall": "lefthook install",
		"setup:security": "safe-chain setup",
		"dev": "vite dev",
		"dev:cloudflare": "pnpm run build && wrangler dev --remote",
		"build:images": "tsx scripts/generate-webp-images.ts",
		"build": "vite build",
		"deploy": "pnpm run build && wrangler deploy",
		"preview": "wrangler dev .svelte-kit/cloudflare/_worker.js --port 4173",
		"preview:ci": "wrangler d1 execute tasks-db --local --file=./drizzle/migrations/0000_nervous_thor_girl_ci.sql && wrangler dev --local .svelte-kit/cloudflare/_worker.js --port 4173",
		"preview:normal": "vite preview",
		"preview:stop": "pnpm exec kill-port 4173 || true",
		"prepare": "pnpm run paraglide:compile && pnpm run types && svelte-kit sync",
		"paraglide:compile": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide --emit-ts-declarations",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "svelte-kit sync && prettier --check . && pnpm run lint:eslint",
		"lint:eslint": "eslint . --cache --cache-strategy content",
		"format": "prettier --write .",
		"test:unit": "vitest",
		"test": "pnpm run test:unit --run && pnpm run test:e2e",
		"test:e2e": "playwright test",
		"types": "rm -f .svelte-kit/cloudflare/_worker.* && wrangler types",
		"lefthook:install": "lefthook install",
		"lefthook:uninstall": "lefthook uninstall",
		"lefthook:commit": "lefthook run pre-commit",
		"lefthook:push": "lefthook run pre-push",
		"check-commit-message": "tsx scripts/check-commit-message.ts",
		"prevent-main-commit": "tsx scripts/prevent-main-commit.ts",
		"git": "tsx scripts/git-workflow.ts",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"version:minor": "npm version minor --no-git-tag-version --loglevel=error"
	},
	"devDependencies": {
		"@aikidosec/safe-chain": "^1.4.2",
		"@chromatic-com/storybook": "^5.0.0",
		"@eslint/compat": "^2.0.2",
		"@eslint/js": "^9.20.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.7.1",
		"@inlang/paraglide-js": "^2.10.0",
		"@libsql/client": "^0.17.0",
		"@playwright/test": "^1.58.2",
		"@storybook/addon-a11y": "^10.2.7",
		"@storybook/addon-docs": "^10.2.7",
		"@storybook/addon-svelte-csf": "^5.0.10",
		"@storybook/addon-vitest": "^10.2.7",
		"@storybook/sveltekit": "^10.2.7",
		"@stylistic/eslint-plugin": "^5.7.1",
		"@sveltejs/adapter-cloudflare": "^7.2.6",
		"@sveltejs/adapter-node": "^5.5.2",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25.2.2",
		"@vitest/browser-playwright": "^4.0.18",
		"@vitest/coverage-v8": "^4.0.18",
		"cspell": "^9.6.4",
		"drizzle-kit": "^0.31.8",
		"drizzle-orm": "^0.45.1",
		"eslint": "^9.20.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-sonarjs": "^3.0.6",
		"eslint-plugin-storybook": "^10.2.7",
		"eslint-plugin-svelte": "^3.14.0",
		"eslint-plugin-unicorn": "^62.0.0",
		"globals": "^17.3.0",
		"kill-port": "^2.0.1",
		"lefthook": "2.1.0",
		"mdsvex": "^0.12.6",
		"playwright": "^1.58.2",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"storybook": "^10.2.7",
		"svelte": "^5.50.0",
		"svelte-check": "^4.3.6",
		"tailwindcss": "^4.1.18",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vite": "^7.3.1",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.18",
		"vitest-browser-svelte": "^2.0.2",
		"wrangler": "^4.63.0"
	},
	"dependencies": {
		"@libsql/client": "^0.17.0",
		"@supabase/ssr": "^0.8.0",
		"@supabase/supabase-js": "^2.95.3",
		"better-auth": "^1.4.18",
		"drizzle-orm": "^0.45.1",
		"sharp": "^0.34.5"
	},
	"pnpm": {
		"overrides": {
			"esbuild": "^0.25.0",
			"cookie": "^0.7.0",
			"undici": "^7.18.2",
			"lodash": ">=4.17.23"
		},
		"onlyBuiltDependencies": [
			"esbuild",
			"lefthook",
			"sharp",
			"unrs-resolver",
			"workerd"
		]
	}
}
